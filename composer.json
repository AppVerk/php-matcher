{
    "name": "defrag/json-matcher",
    "type": "library",
    "keywords": ["json", "matcher"],
    "license": "MIT",    
    "require": {
        "php": ">=5.3.0",
        "symfony/property-access": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "JsonMatcher": "src/",
            "JsonMatcher\\Tests": "tests/"
        },
        "files": ["match.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
