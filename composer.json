{
    "name": "coduo/php-matcher",
    "type": "library",
    "keywords": ["json", "matcher", "tests", "match"],
    "license": "MIT", 
    "authors": [
        {
            "name": "Michał Dąbrowski",
            "email": "dabrowski@brillante.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-filter": "*",
        "coduo/php-to-string": "^2",
        "symfony/property-access": "^2.3|^3.0",
        "symfony/expression-language": "^2.3|^3.0",
        "doctrine/lexer": "1.0.*",
        "openlss/lib-array2xml": "0.0.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-0": {
            "Coduo\\PHPMatcher": "src/",
            "Coduo\\PHPMatcher\\Tests": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
