{
    "name": "defrag/php-matcher",
    "type": "library",
    "keywords": ["json", "matcher", "tests"],
    "license": "MIT", 
    "authors": [
        {
            "name": "Michał Dąbrowski",
            "email": "dabrowski@brillante.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/property-access": "~2.3",
        "symfony/expression-language": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "PHPMatcher": "src/",
            "PHPMatcher\\Tests": "tests/"
        },
        "files": ["match.php"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
